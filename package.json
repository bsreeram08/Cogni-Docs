{
  "name": "documentation-mcp",
  "version": "1.0.0",
  "description": "A Model Context Protocol server for AI assistants to search and query documentation using Google Cloud services",
  "type": "module",
  "private": true,
  "main": "src/mcp-server.ts",
  "scripts": {
    "dev": "unset GOOGLE_APPLICATION_CREDENTIALS && bun run --watch src/upload-server.ts",
    "start": "bun run src/mcp-server.ts",
    "upload-server:prod": "bun run src/upload-server.ts",
    "web:dev": "cd web && bun run dev",
    "web:build": "cd web && bun run build",
    "web:preview": "cd web && bun run preview",
    "mcp-server": "bun run src/mcp-server.ts",
    "typecheck": "tsc --noEmit",
    "lint": "tsc --noEmit",
    "clean": "rm -rf dist",
    "test": "echo \"No tests specified\" && exit 0"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "documentation",
    "ai",
    "vector-search",
    "google-cloud",
    "firestore",
    "vertex-ai",
    "gemini"
  ],
  "dependencies": {
    "@elysiajs/cors": "^1.3.3",
    "@google-cloud/firestore": "^7.11.3",
    "@google-cloud/vertexai": "^1.10.0",
    "@modelcontextprotocol/sdk": "^1.2.0",
    "@types/html-to-text": "^9.0.4",
    "elysia": "^1.3.9",
    "html-to-text": "^9.0.5",
    "unpdf": "^0.11.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "typescript": "^5.6.3"
  },
  "engines": {
    "bun": ">=1.0.0"
  },
  "repository": {
    "type": "git",
    "url": "."
  },
  "license": "MIT"
}
